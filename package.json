{
  "name": "sql-da",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run build:js",
    "build:types": "tsc",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "dev": "cross-env NODE_ENV=development node_modules/.bin/nodemon --watch src --exec node_modules/.bin/ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development node dist/main.js",
    "start:test": "cross-env NODE_ENV=test node dist/main.js",
    "start:production": "cross-env NODE_ENV=production node dist/main.js",
    "start:production-test": "cross-env NODE_ENV=production-test PROD_ENV=test node dist/main.js",
    "test:dev": "npm run lint && npm run build:types && npm run build:js && cross-env NODE_ENV=development.test ts-node -r tsconfig-paths/register test",
    "test:test": "npm run lint && npm run build:types && npm run build:js && cross-env NODE_ENV=test.test ts-node -r tsconfig-paths/register test",
    "test:production": "npm run lint && npm run build:types && npm run build:js && cross-env NODE_ENV=production ts-node -r tsconfig-paths/register test",
    "test:production-test": "npm run lint && npm run build:types && npm run build:js && cross-env NODE_ENV=production-test.test ts-node -r tsconfig-paths/register test",
    "model": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register --transpile-only createModel.ts",
    "lint": "eslint --ext \".js,.ts\" .",
    "lint:fix": "eslint --fix --ext \".js,.ts\" .",
    "build:typedoc": "typedoc",
    "dev:typedoc": "typedoc --watch --preserveWatchOutput"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "axios": "^1.2.1",
    "chardet": "^1.5.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "iconv-lite": "^0.6.3",
    "ipaddr.js": "^2.0.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "request-ip": "^3.3.0",
    "sequelize": "^6.25.8",
    "tedious": "^11.0.8",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-flow-strip-types": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/prettier": "^2.7.1",
    "@types/request-ip": "^0.0.37",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  }
}
